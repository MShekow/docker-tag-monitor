{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "description": "Automerge Poetry updates",
      "matchPackageNames": ["poetry"],
      "matchManagers": ["pip_requirements "],
      "automerge": true
    }
  ],
}
